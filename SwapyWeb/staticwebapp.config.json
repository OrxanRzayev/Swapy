{
    "trailingSlash": "auto",
    "routes": [
    {
        "route": "/auth*"
    },
    {
        "route": "/chats",
        "allowedRoles": ["authenticated"]
    },
    {
        "route": "/products/*"
    },
    {
        "route": "/settings",
        "allowedRoles": ["authenticated"]
    },
    {
        "route": "/shops*"
    },
    {
        "route": "/users/*"
    }],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "responseOverrides": {
        "401": {
            "redirect": "/auth/login",
            "statusCode": 302
        },
        "404": {
            "redirect": "/404",
            "statusCode": 302
        }
    },
    "mimeTypes": {
        ".json": "text/json"
    }
}